{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "installCommand": "npm install --no-save --legacy-peer-deps",
  "devCommand": "npm start",
  "outputDirectory": "dist",
  "rewrites": [
    { 
      "source": "/(.*)",
      "destination": "/index.html" 
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "REACT_APP_API_URL": "${REACT_APP_API_URL}"
  }
}
